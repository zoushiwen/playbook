apiVersion: v1
kind: Service
metadata:
  labels:
    app: etcd-svc
  name: etcd
  namespace: {{project_namespace|default('wcloud')}}
spec:
  ports:
  - name: etcd-client
    port: 2379
    targetPort: 2379
  - name: etcd-peer
    port: 2380
    targetPort: 2380
  selector:
    app: {{etcd_service_name}}
  type: ClusterIP


